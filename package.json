{
  "name": "vuejs-gitfinder",
  "version": "1.0.0",
  "description": "A Vue.js project",
  "author": "Gustavo <gustavonvp@gmail.com>",
  "private": true,
  "main": "main.js",
  "target": "esnext",
  "type": "commonjs",
  "scripts": {
    "server": "karma start test\\unit\\karma.conf.js --log-level debug --single-run",
    "format": "f -w",
    "format:check": "f",
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "dev1": "webpack serve --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node node_modules/@babel/preset-env test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit test/e2e/specs",
    "build1": "npm webpack build --config ./webpack.config.js --stats verbose",
    "dev": "cross-env NODE_OPTIONS=--openssl-legacy-provider",
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider"
  },
