{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-2"
  ],
  "plugins": ["transform-vue-jsx", "transform-runtime"],
  "env": {
    "test": {
      "presets": ["es2015", "env", "stage-2"],
      "plugins": ["transform-vue-jsx", "istanbul", "transform-es2015-arrow-functions", "transform-async-to-generator"]
    }
  }
}
